	<settings name="settings">
		<set name="interval">30</set>
		<set name="max_ping">600</set>
		<set name="max_ping_duration">60</set>
	</settings>
	<settings name="commands">
		<!-- The command level for !ci -->
		<set name="ci">20</set>
	</settings>
</configuration>