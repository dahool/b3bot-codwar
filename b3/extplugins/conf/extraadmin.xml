<configuration plugin="extraadmin">
    <settings name="settings">
      <set name="announce">2</set>
      <set name="load_location">/local/codwar/q3a/q3ut4/configs.list</set>
      <set name="min_maps_level">90</set>
      <set name="auto_balance">True</set>
      <set name="min_reg_connections">50</set>
      <set name="super_reg_level">90</set>
      <set name="load_match_only">false</set>
    </settings>

    <settings name="commands">
      <set name="pamap-map">40</set>
      <set name="pasetnextmap-setnextmap">40</set>
      <set name="pashuffleteams-shuffleteams">20</set>
      <set name="pasetpassword">60</set>
      <set name="pasetteamname-setteamname">80</set>
      <set name="pabandetail-bandetail">80</set>
      <set name="paunreg-unreg">40</set>
      <set name="paload">40</set>
      <set name="paslapall-slapall">80</set>
      <set name="groups">90</set>
      <set name="pamaps-maps">2</set>
      <set name="makereg-mr">40</set>
      <set name="who">1</set>
      <set name="clientinfo-cinfo">40</set>
      <set name="aliases-alias">20</set>
    </settings>

    <settings name="maprotation">
      <set name="enable">True</set>
      <set name="use_random">True</set>
    </settings>

	<settings name="messages">
		<set name="clientinfo">^7[^2$name^7] id: (^3@$id^7), IP: (^3@$ip^7), primera visita: ^3$timeAdd^7, ultima visita: ^3$lastVisit^7, grupo: ^2$group^7, conecciones: ^2$connections^7, advertencias ^2$numWarnings^7, bans ^2$numBans</set>
	</settings>  
  
</configuration>
