<configuration plugin="ipdb2">
    <settings name="settings">
        <set name="baninfodump">7</set> <!-- FIXED HOUR EJ: RUN EVERY DAY AT 7AM UTC || -1 DISABLE -->
        <set name="baninfointerval">2</set> <!-- EVERY 2 HOURS  || 0 DISABLE -->
        <set name="interval">5</set> <!-- EVERY 5 MINUTES -->
        <!--
		remote permissions
		0 = disable remote actions
		1 = ban
		2 = unban
		4 = add notice
		8 = remove notice
		add numbers, 15 = all
		-->
		<set name="remotepermission">15</set>
        <set name="key">--------</set>
    </settings>
    <settings name="commands">
        <set name="userlink">2</set>
        <set name="dbaddnote">40</set>
        <set name="dbdelnote">40</set>
        <set name="dbclearban">60</set>
    </settings>
   	
</configuration>
